class Vehicle:
 def __init__(self, name, mileage, capacity):
    self.name = name
    self.mileage = mileage
    self.capacity = capacity
    
 def fare(self):
    x = self.capacity * 100 
    return x + 0.1*x

class Bus(Vehicle):
  pass

School_bus = Bus("School Volvo", 12, 50)
print("Total Bus fare is:", School_bus.fare())
